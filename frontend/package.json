    "@mui/material": "^7.2.0",
    "@mui/x-data-grid": "^8.9.2",
    "@mui/x-tree-view": "^8.9.2",
    "@openai/codex": "^0.23.0",
    "@tanstack/react-query": "^5.85.3",
    "axios": "^1.11.0",
    "chart.js": "^4.5.0",
    "dayjs": "^1.11.13",
    "ky": "^1.8.2",
    "lucide-react": "^0.539.0",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-mui-sidebar": "^1.6.3",
    "react-router-dom": "^7.7.1",
    "recharts": "^3.1.2",
    "uuid": "^11.1.0"
