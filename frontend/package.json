{
  "name": "isms-frontend-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 9001",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "openapi:export": "python3 ../backend/scripts/export_openapi.py",
    "openapi:gen": "orval --config orval.config.cjs",
    "openapi:gen:alt": "openapi --input src/api/openapi.json --output src/api/client --client fetch",
    "openapi:update": "npm run openapi:export && npm run openapi:gen:alt"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
